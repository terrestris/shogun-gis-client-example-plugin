    "@module-federation/enhanced": "^0.2.3",
    "@rspack/cli": "^0.7.5",
    "@rspack/core": "^0.7.5",
    "@rspack/plugin-react-refresh": "^0.7.5",
    "@swc/helpers": "^0.5.11",
